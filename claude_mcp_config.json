{
  "mcpServers": {
    "vectorstore": {
      "command": "python",
      "args": ["src/mcp_services/langchain_vector"],
      "env": {
        "GOOGLE_API_KEY": "ENV_GOOGLE_API_KEY",
        "MONGODB_USER": "ENV_MONGODB_USER",
        "MONGODB_PASSWORD": "ENV_MONGODB_PASSWORD",
        "MONGODB_URL": "ENV_MONGODB_URL",
        "MONGODB_PORT": "ENV_MONGODB_PORT",
        "MONGODB_PARAMS": "ENV_MONGODB_PARAMS",
        "MONGODB_NAME": "ENV_MONGODB_NAME",
        "MONGODB_COLLECTION_NAME": "contracts",
        "EMBEDDING_MODEL": "ENV_EMBEDDING_MODEL"
      }
    },
  }
}
